programa {
  funcao inicio() {
    cadeia nomeCidadeMoradia, nomeCidadeEstudo
    escreva ("\nQual a cidade em que mora?")
    leia(nomeCidadeMoradia)
    escreva ("\n\nQual a cidade em que estuda?")
    leia (nomeCidadeEstudo)
    escreva ("\nMoradia: " + nomeCidadeMoradia + "\nEstudo: " + nomeCidadeEstudo )
  }
}
